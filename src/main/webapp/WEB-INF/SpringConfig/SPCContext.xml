<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:aop="http://www.springframework.org/schema/aop"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-4.0.xsd">
  
  <bean id="spcDimensionDAO" class="org.amc.dao.SPCMeasurementDAO" scope="prototype">
  </bean>
  
  <bean id="spcPartsListDAO" class="org.amc.dao.DAO" scope="prototype">
     <constructor-arg>  	
     	<value type="java.lang.Class">org.amc.model.spc.SPCPartsList</value>
     </constructor-arg>
  </bean>
  
  <bean id="spcDataDAO" class="org.amc.dao.SPCDataDAO" scope="prototype"></bean> 
  </beans>